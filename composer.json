{
    "name": "lara-care/cv-agent",
    "description": "Laravel package to generate CV sections with AI",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "Jery Foto",
            "email": "jeryfoto@gmail.com",
            "homepage": "https://jery-portal.vrecel.app/"
        }
    ],
    "autoload": {
        "psr-4": {
            "LaraCare\\CvAgent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraCare\\CvAgent\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraCare\\CvAgent\\Providers\\CvAgentProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "google-gemini-php/client": "^2.5"
    },
    "require-dev": {},
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}